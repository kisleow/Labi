# include <stdio.h>
# include <math.h>

int main()

{
float x0, y0, z0;
printf("x0, y0, z0: ");
scanf("%f %f %f", &x0, &y0, &z0);
float r;
printf("r: ");
scanf("%f", &r);
float x, y, z;
printf("x, y, z: ");
scanf("%f %f %f", &x, &y, &z);

if ((x0 - x) * (x0 - x) + (y0 - y) * (y0 - y) + (z0 - z) * (z0 - z) <= r * r)
puts("yes");
else
puts("no");

return 0;
}
